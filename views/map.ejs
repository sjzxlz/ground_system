<% include header.ejs %>
<div class="container-fluid">
    <div id="map"></div>
    <script>
        var map = L.map('map').setView([36, 103.6], 8);

        L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', {
            attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors'
        }).addTo(map);

        L.marker([36, 103.6]).addTo(map)
            .bindPopup('兰州市.<br> 城关区.')
            .openPopup();
    </script>
</div>
